{
  "name": "mvvm-vue",
  "version": "1.0.0",
  "description": "MVVM-Vue",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean:build && webpack",
    "clean:build": "npm run rimraf -- ./dist",
    "rimraf": "rimraf",
    "commit": "git cz",
    "dev": "rollup -c --watch",
    "reload": "livereload 'dist/' -d",
    "watch": "npm-run-all --parallel reload dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djkloop/MVVM-Vue.git"
  },
  "keywords": [
    "MVVM-Vue"
  ],
  "author": "djkloop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djkloop/MVVM-Vue/issues"
  },
  "homepage": "https://github.com/djkloop/MVVM-Vue#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "cz-conventional-changelog": "^2.1.0",
    "livereload": "^0.7.0",
    "moment": "^2.22.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "browserslist": [
    "ie >= 9",
    "iOS >= 7",
    "Android > 4.1",
    "Firefox > 20",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "rollup-plugin-typescript": "^0.8.1"
  }
}
